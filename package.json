  "license": "MIT",
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.{js,jsx,json,html,md,scss}": ["prettier --write", "git add"],
    "*.{js,jsx}": ["eslint --fix", "git add"]
  },
  "scripts": {
    "clean": "rimraf _dist",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .jsx,.js .",
    "lint:staged": "lint-staged",
    "build": "npm run clean && cross-env NODE_ENV=production npm run webpack",
    "webpack":
      "node -r @babel/register ./node_modules/webpack/bin/webpack.js -p --config=webpack.config.babel.js"
  },
  "dependencies": {
    "autoprefixer": "^7.2.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "postcss-import": "^11.0.0",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "@babel/preset-react": "^7.0.0-beta.37",
    "@babel/preset-stage-2": "^7.0.0-beta.37",
    "@babel/register": "^7.0.0-beta.37",
    "babel-eslint": "^8.2.1",
    "babel-loader": "8.0.0-beta.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.6",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.10",
    "pre-commit": "^1.2.2",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6"
  }
}
